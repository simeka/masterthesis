    journal = {PLoS Comput Biol},
    publisher = {Public Library of Science},
    title = {Models of Neocortical Layer 5b Pyramidal Cells Capturing a Wide Range of Dendritic and Perisomatic Active Properties},
    year = {2011},
    month = {07},
