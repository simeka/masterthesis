	number = "429",
	title = "{A framework for plasticity implementation on the SpiNNaker neural architecture}",
	url = "http://www.frontiersin.org/neuromorphic_engineering/10.3389/fnins.2014.00429/abstract",
	volume = "8",
	year = "2015"
}

